# Environment Configuration
NODE_ENV=development  # Set to 'production' when deploying
# Example .env file
RPC_KEY="https://devnet.helius-rpc.com?api-key=<YOUR_API_KEY>"
# Blockchain Network Settings
THETA_NETWORK=https://mainnet.theta.network  # URL for Theta network
THETA_API_KEY=your-theta-api-key  # API key for accessing Theta blockchain services

# Token and Smart Contract Settings
THETA_CONTRACT_ADDRESS=0xYourContractAddress  # Address of the smart contract on Theta
THETA_TOKEN_SYMBOL=YOURTOKEN  # Symbol for your custom token

# Authentication Settings
AUTH0_DOMAIN=your-auth0-domain  # Auth0 domain for authentication
AUTH0_CLIENT_ID=your-auth0-client-id  # Auth0 client ID
AUTH0_CLIENT_SECRET=your-auth0-client-secret  # Auth0 client secret

# API Endpoints
API_URL=https://api.example.com  # Base URL for your API
FRONTEND_URL=https://frontend.example.com  # URL of your frontend application

# Database Configuration (if applicable)
DB_HOST=localhost  # Database hostname
DB_PORT=5432  # Database port
DB_USER=myuser  # Database username
DB_PASSWORD=mypassword  # Database password
DB_NAME=mydatabase  # Database name

# External API Keys
GOOGLE_MAPS_API_KEY=your-google-maps-api-key  # API key for Google Maps
SENDGRID_API_KEY=your-sendgrid-api-key  # API key for SendGrid

# Webhooks (if applicable)
WEBHOOK_SECRET=your-webhook-secret  # Secret for validating webhook requests

# Deployment Settings
PORT=5000  # Port number for your server

# Miscellaneous
REACT_APP_ENVIRONMENT=development  # Environment identifier for React
REACT_APP_FEATURE_FLAG=true  # Feature flag for enabling/disabling features

# Security and Privacy
JWT_SECRET=your-jwt-secret  # Secret for JWT token encryption
SESSION_SECRET=your-session-secret  # Secret for session management
